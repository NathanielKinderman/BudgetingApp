
@{
    ViewBag.Title = "Map";
}

<h2>Map</h2>
<style>
    /* Always set the map height explicitly to define the size of the div
       * element that contains the map. */
    #map {
         height: 400px;
         width: 400px;
    }
   
   
</style>
<div class="row">
    <div id="map"></div>
    <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: 43.0389, lng: -87.9065},
          zoom: 8
        });
      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBeWLVk14n3OzLLKLvDvUyWspH929EcEaY&callback=initMap"
            async defer></script>
</div>

